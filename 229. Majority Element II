class Solution {
public:
    vector<int> majorityElement(vector<int>& nums) {
        unordered_map<int, int> freq;
        vector<int> res;
        int n = nums.size();

        for(int n : nums){
            freq[n]++;
        }

        for(auto& pair : freq){
            if(pair.second > n/3){
                res.push_back(pair.first);
            }
        }
        return res;
    }
};
