class Solution {
public:
    int findMaxConsecutiveOnes(vector<int>& nums) {
        int maxC = 0, count = 0;
        for(int n : nums){
            if(n == 1){
                count++;
            } else {
                count = 0;
            }
            maxC = max(maxC, count);
        }
        return maxC;
    }
};
